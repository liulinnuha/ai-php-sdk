{
    "name": "liulinnuha/ai-php-sdk",
    "description": "Universal AI SDK for PHP multiple providers like OpenAI, DeepSeek, Gemini, etc.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AiPhpSdk\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "authors": [
        {
            "name": "Ulin Nuha",
            "email": "h3artscode@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AiPhpSdk\\Laravel\\AiPhpSdkServiceProvider"
            ],
            "aliases": {
                "AiSdk": "AiPhpSdk\\Laravel\\AiPhpSdk"
            }
        }
    }
}
