{
    "name": "liulinnuha/ai-php-sdk",
    "description": "Universal AI SDK for PHP multiple providers like OpenAI, DeepSeek, Gemini, etc.",
    "keywords": [
        "ai",
        "sdk",
        "php",
        "openai",
        "deepseek",
        "gemini",
        "chatgpt",
        "embedding",
        "ai php sdk",
        "ai php sdk openai",
        "ai php sdk deepseek",
        "ai php sdk gemini",
        "ai php sdk chatgpt"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "AiPhpSdk\\": "src/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "authors": [
        {
            "name": "Ulin Nuha",
            "email": "h3artscode@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
